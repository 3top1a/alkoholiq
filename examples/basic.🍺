// No arrays for now

x = 1
y = 2

fn + a b {
    ret = 0
    copy a ret
    copy b ret
    return ret
}

z = +( x y)
goto(z)
basm(".")


---


set x
> (alloc) + (add)

set y
> (alloc) ++ (add)


function call
allocate z
> (alloc)
new stack
>

set a
> (alloc) (copy)

set b
> (alloc) (copy)

copy a
copy b
return ret
(move ret to z)

pop stack
(zero b)
(zero a)

goto z
< (goto z)
print
.
