set f_n-1 1
set f_n-2 1
set n 10
set one 1

printc f_n-1
print_msg " "
printc f_n-2
print_msg " "

dec_by n 2

while_nz n
    copy f_n-1 f_n
    add f_n f_n-2

    // Printc will pretty print numbers
    printc f_n

    if_neq n one
        print_msg ' '
    end

    copy f_n-2 f_n-1
    copy f_n f_n-2

    dec n
end
// Also include a new line; '' and "" are equivalent
print_msg '\n'
